+# You should have received a copy of the GNU Affero General Public License
+# along with this program.  If not, see <http://www.gnu.org/licenses/>.
+
+import django.forms
+
+class UsernameForm(django.forms.Form):
+    secret_word = django.forms.CharField(error_messages={'required': 'No username was given.'})
+
+class UsernameNextForm(django.forms.Form):
+    secret_word = django.forms.CharField(error_messages={'required': 'No username was given.'})
+
+class TopForm(django.forms.Form):
+    top = django.forms.CharField(error_messages={'required': 'No bash top output was entered.'}, widget=django.forms.Textarea())
+
+class BashHelpForm(django.forms.Form):
+    secret_word = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'})
+    extra_credit = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'})
+
+class FilesForm(django.forms.Form):
+    man_ls = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'3'}))
+    bash_cd = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'})
+    bash_pwd = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'2'}))
+    bash_chmod = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'2'}))
+    bash_own = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'6'}))
+    bash_chgrp = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'3'}))
+    bash_rm = django.forms.CharField(error_messages={'required': 'An answer is required. Try again.'}, widget=django.forms.Textarea(attrs={'rows':'2'}))
+
