+    <a name="bashusername-form"></a>
+    {% if bash_username_error_message %}
+    <ul class="errorlist"><li>{{ bash_username_error_message }}</li></ul>
+    {% endif %}
+    {{ bash_username_form.secret_word.errors }}
+    <p>Based on the break-down of the shell prompt above, what is your
+    username on your terminal?</p> 
+    <input type="hidden" value="{{bash_stored_username}}" name="secret_word" />
+    <br style="clear: left;" />
+    <input type="submit" value="Submit" style="float: right; margin-top:-25px;margin-right: 230px;" />
+  </div>
+</form>
+{# <p class="next_mission_link">
+  <a href="{% url mysite.missions.bash.views.top %}">Right! Now the next mission - Using Top. &raquo;</a></p>
+</div> #}
+{% else %}
+<form method="post" action="{% url mysite.missions.bash.views.username_submit %}#bashusername-form">
+  <div class="form-row">
+    <a name="bashusername-form"></a>
