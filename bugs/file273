+
+from mysite.missions.base.controllers import *
+
+class BashRepository(object):
+    
+    def __init__(self, username):
+        self.username = username
+        
+class BashTopMission(object):
+
+    @classmethod
+    def validate_top(cls, username, top):
+        EXPECTED_TOP_LINE = 'Cpu(s):'
+        success_count = top.find(EXPECTED_TOP_LINE)
+        if success_count != -1:
+            return True
+        else:
+            return False
+
+    @classmethod
+    def store_bash_username(cls, bash_stored_username):
+        return bash_stored_username
