+# This file is part of OpenHatch.
+# Copyright (C) 2010, 2011 OpenHatch, Inc.
+#
+# This program is free software: you can redistribute it and/or modify
+# it under the terms of the GNU Affero General Public License as published by
+# the Free Software Foundation, either version 3 of the License, or
+# (at your option) any later version.
+#
+# This program is distributed in the hope that it will be useful,
+# but WITHOUT ANY WARRANTY; without even the implied warranty of
+# MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
+# GNU Affero General Public License for more details.
+#
+# You should have received a copy of the GNU Affero General Public License
+# along with this program.  If not, see <http://www.gnu.org/licenses/>.
+
+from mysite.missions.base.views import *
+from mysite.missions.bash import controllers, forms
+
+
+### POST handlers
+###
+### Forms submit to this, and we use these to validate input and/or
+### modify the information stored about the user, such as recording
+### that a mission was successfully completed.
+
+@login_required
+def reset(request):
+    if request.method != 'POST':
+        return HttpResponseNotAllowed(['POST'])
+    controllers.unset_mission_completed(request.user.get_profile(), 'bash_username')
+    controllers.unset_mission_completed(request.user.get_profile(), 'bash_top')
+    controllers.unset_mission_completed(request.user.get_profile(), 'bash_help')
+    controllers.unset_mission_completed(request.user.get_profile(), 'bash_files')
+    if 'stay_on_this_page' in request.GET:
+        return HttpResponseRedirect(reverse(main_page))
+    else:
+        return HttpResponseRedirect(reverse(username))
+
+@login_required
+def username_submit(request):
+    # Initialize data array and some default values.
+    data = {}
+    data['bash_username_form'] = forms.UsernameForm()
+    data['bash_username_error_message'] = ''
+    if request.method == 'POST':
+        form = forms.UsernameForm(request.POST)
+        if form.is_valid():
+            new_username_for_bash = form.cleaned_data['secret_word'].lower()
+            controllers.set_mission_completed(request.user.get_profile(), 'bash_username')
+            data['bash_stored_username'] = new_username_for_bash 
+            return HttpResponseRedirect(reverse(username))
+        data['bash_username_form'] = form
+    return username(request, data)
+
+@login_required
+def username_next_submit(request):
+    # Initialize data array and some default values.
+    data = {}
+    data['bash_usernamenext_form'] = forms.UsernameNextForm()
+    data['bash_usernamenext_error_message'] = ''
+    if request.method == 'POST':
+        form = forms.UsernameNextForm(request.POST)
+        if form.is_valid():
+            new_username_for_bash = form.cleaned_data['secret_word'].lower()
+            return HttpResponseRedirect(reverse(top)) + urllib.urlencode({'bash_username': new_username_for_bash }) 
+        data['bash_usernamenext_form'] = form
+    return top(request, data)
+
+@login_required
+def top_submit(request):
+    # Initialize data array and some default values.
+    data = {}
+    data['bash_top_form'] = forms.TopForm()
+    data['bash_top_error_message'] = ''
+    if request.method == 'POST':
+        form = forms.TopForm(request.POST)
+        if form.is_valid():
+                if controllers.BashTopMission.validate_top(request.user.username, form.cleaned_data['top']):
+                    controllers.set_mission_completed(request.user.get_profile(), 'bash_top')
+                    return HttpResponseRedirect(reverse(top))
+                else:
+                    data['bash_top_error_message'] = "The top output is incorrect.  Give it another try!"
+        data['bash_top_form'] = form
+    return top(request, data)
+
+@login_required
+def bash_help_submit(request):
+    # Initialize data array and some default values.
+    data = {}
+    data['bash_help_form'] = forms.BashHelpForm()
+    data['bash_help_error_message'] = ''
+    if request.method == 'POST':
+        form = forms.BashHelpForm(request.POST)
+        if form.is_valid():
+            Extra_Credit = form.cleaned_data['extra_credit'].lower()
+            success_count = Extra_Credit.find('delay time') 
+            if (form.cleaned_data['secret_word'].lower() == 'n' and success_count != -1):
+                controllers.set_mission_completed(request.user.get_profile(), 'bash_help')
+                return HttpResponseRedirect(reverse(bash_help_page))
+            else:
+                data['bash_help_error_message'] = "The answer that you have given is incorrect."
+        data['bash_help_form'] = form
+    return bash_help_page(request, data)
+
+@login_required
+def files_submit(request):
+    # Initialize data array and some default values.
+    data = {}
+    data['bash_file_form'] = forms.FilesForm()
+    data['bash_file_error_message'] = ''
+    if request.method == 'POST':
+        form = forms.FilesForm(request.POST)
+        if form.is_valid():
+            Man_Ls = form.cleaned_data['man_ls'].lower()
+            ls_success_count = Man_Ls.find('1')
+            Bash_Cd = form.cleaned_data['bash_cd'].lower()
+            Cd_success_count = Bash_Cd.find('change directory')
+            Bash_Pwd = form.cleaned_data['bash_pwd'].lower()
+            Pwd_success_count = Bash_Pwd.find('3')
+            Bash_Chmod = form.cleaned_data['bash_chmod'].lower()
+            Chmod_success_count = Bash_Chmod.find('4')
+            Bash_Own = form.cleaned_data['bash_own'].lower()
+            Chown_success_count = Bash_Own.find('5')
+            Bash_Chgrp = form.cleaned_data['bash_chgrp'].lower()
+            Chgrp_success_count = Bash_Chgrp.find('6')
+            Bash_Rm = form.cleaned_data['bash_rm'].lower()
+            Rm_success_count = Bash_Rm.find('7')
+            if (ls_success_count != -1 and Cd_success_count != -1 and Pwd_success_count != -1 and 
+                Chmod_success_count != -1 and Chown_success_count != -1 and 
+                Chgrp_success_count != -1 and Rm_success_count != -1):
+                controllers.set_mission_completed(request.user.get_profile(), 'bash_files')
+                return HttpResponseRedirect(reverse(files))
+            else:
+                data['bash_file_error_message'] = "The answers you have given are incorrect. Please try again!"
+        data['bash_file_form'] = form
+    return files(request, data)
+
+### State manager
+class BashMissionPageState(MissionPageState):
+    def __init__(self, request, passed_data):
+        super(BashMissionPageState, self).__init__(request, passed_data, 'Using Bash')
+
+    def as_dict_for_template_context(self):
+        (data, person) = self.get_base_data_dict_and_person()
+        if person:
+            data.update({
+                'bash_username_done': controllers.mission_completed(person, 'bash_username'),
+                'bash_top_done': controllers.mission_completed(person, 'bash_top'),
+                'bash_help_done': controllers.mission_completed(person, 'bash_help'),
+                'bash_files_done': controllers.mission_completed(person, 'bash_files'),
+            })
+        return data
+
+
+
+### Normal GET handlers. These are usually pretty short.
+
+@view
+def main_page(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'Start page'
+    return (request, 'missions/bash/main_page.html',
+            state.as_dict_for_template_context())
+
+@view
+def long_description(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'About Bash'
+    return (request, 'missions/bash/about_bash.html',
+            state.as_dict_for_template_context())
+
+@view
+def basics(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'Basics'
+    data = state.as_dict_for_template_context()
+    return (request, 'missions/bash/basics.html', data)
+
+@login_required
+@view
+def username(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'Username'
+    data = state.as_dict_for_template_context()
+    data['bash_username_form'] = forms.UsernameForm()
+    return (request, 'missions/bash/username.html', data)
+
+@login_required
+@view
+def top(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'Using Top'
+    state.mission_step_prerequisite = 'bash_username'
+    data = state.as_dict_for_template_context()
+    data['bash_top_form'] = forms.TopForm()
+    return (request, 'missions/bash/top.html', data)
+
+@login_required
+@view
+def bash_help_page(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'Getting Help'
+    state.mission_step_prerequisite = 'bash_top'
+    data = state.as_dict_for_template_context()
+    data['bash_help_form'] = forms.BashHelpForm()
+    return (request, 'missions/bash/help.html', data)
+
+@login_required
+@view
+def files(request, passed_data = None):
+    state = BashMissionPageState(request, passed_data)
+    state.this_mission_page_short_name = 'The Filesystem'
+    state.mission_step_prerequisite = 'bash_help'
+    data = state.as_dict_for_template_context()
+    data['bash_file_form'] = forms.FilesForm()
+    return (request, 'missions/bash/files.html', data)
